# Mount point	fs	        device                       [device2]
/system		ext4	        /dev/block/mmcblk0p3
/cache		ext4	        /dev/block/mmcblk0p4
/data		ext4	        /dev/block/mmcblk0p5          length=-16384
/boot		emmc			/dev/block/mmcblk0p15
/recovery	emmc	        /dev/block/mmcblk0p15
/efs		ext4	        /dev/block/mmcblk0p7
/preload    ext4			/dev/block/mmcblk0p9
/sdcard1		vfat	 /dev/block/mmcblk1p1     /dev/block/mmcblk1
/sdcard          vfat          /dev/block/mmcblk0p8
